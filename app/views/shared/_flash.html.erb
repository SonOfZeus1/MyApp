<% if flash[:warning] %>
    <div class="alert alert-warning mt-4">
    <%= flash[:warning] %>
    </div>
<% end %>

<% if flash[:notice] %>
    <div class="alert alert-info mt-4">
    <%= flash[:notice] %>
    </div>
<% end %>

<% if flash[:success] %>
<div class="alert alert-success mt-4">
    <%= flash[:success] %>
</div>
<% end %>

