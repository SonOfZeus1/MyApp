<h1>Log in</h1>

<%= form_with url: sign_in_path do |form| %>


<div class="mb-3">
    <%=  form.label :email %>
    <%=  form.text_field :email, class:"form-control", placeholder: "steve@gmail.com" %>
</div>


<div class="mb-3">
    <%=  form.label :password %>
    <%=  form.password_field :password, class:"form-control", placeholder: "password"%>
</div>

<div>
    <%= form.submit "Log in", class: "btn btn-primary"%>
</div>


<%end%>